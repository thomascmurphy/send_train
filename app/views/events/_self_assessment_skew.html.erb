<% if @last_self_assessment.present? %>
  <div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title">Last Self-Assessment (<%= @last_self_assessment.start_date.strftime("%b %d, %Y") %>)</h3>
    </div>
    <div class="panel-body">
      <div class="build_skew"
           data-skew-data='<%= @skew_data %>'
           data-chart-title="Self-Assessment"
           data-data-max='<%= @skew_data_max %>'>
      </div>
    </div>
  </div>
<% end %>
